{
    "name": "xrplwin/xumm-sdk-php",
    "description": "A PHP SDK for Xumm by XRPLabs.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Xrpl\\XummSdkPhp\\Tests\\": "tests/",
            "Xrpl\\XummSdkPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Pauline Vos",
            "email": "info@pauline-vos.nl"
        }
    ],
    "keywords": ["xrp","ripple","xrpl","xumm"],
    "homepage": "https://xumm.readme.io/",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "php": "^8.2.0",
        "guzzlehttp/guzzle": "^7.4",
        "symfony/serializer": "^6.0",
        "vlucas/phpdotenv": "^5.4",
        "symfony/property-access": "^6.0",
        "ratchet/pawl": "^0.4.1",
        "monolog/monolog": "^2.3|^3.0",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "behat/behat": "^3.10",
        "squizlabs/php_codesniffer": "dev-master",
        "wyrihaximus/async-test-utilities": "^4.2"
    },
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
